{
  "version": 2,
  "builds": [
    {
      "src": "인덱스.html",
      "use": "@vercel/static"
    },
    {
      "src": "관리자.html",
      "use": "@vercel/static"
    },
    {
      "src": "자산/**",
      "use": "@vercel/static"
    },
    {
      "src": "API/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/인덱스.html"
    },
    {
      "src": "/admin",
      "dest": "/관리자.html"
    },
    {
      "src": "/관리자",
      "dest": "/관리자.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}